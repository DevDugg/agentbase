# AgentBase Environment Variables
# Copy this file to .env and fill in your actual values

# Redis Configuration
REDIS_URL=redis://redis:6379

# Metrics API Configuration
METRICS_PORT=3001
CORS_ORIGIN=*

# ============================================
# Dev Workforce Agents (Backend, DevOps, Frontend)
# ============================================

# Agent Backend - Senior Backend Engineer
BACKEND_API_KEY=sk-ant-api03-your-backend-api-key-here
BACKEND_DISCORD_TOKEN=your-backend-discord-bot-token-here

# Agent DevOps - Senior DevOps Engineer
DEVOPS_API_KEY=sk-ant-api03-your-devops-api-key-here
DEVOPS_DISCORD_TOKEN=your-devops-discord-bot-token-here

# Agent Frontend - Senior Frontend Engineer
FRONTEND_API_KEY=sk-ant-api03-your-frontend-api-key-here
FRONTEND_DISCORD_TOKEN=your-frontend-discord-bot-token-here

# ============================================
# Optional Configuration
# ============================================

# Discord Server ID (for reference)
DISCORD_SERVER_ID=your-server-id-here

# Node Environment
NODE_ENV=production

# Working Directory
WORKING_DIRECTORY=/agent-workspace
